<div class="container">
<div class="panel panel-default">
<div class="panel-body">
<%= form_for @picture, html: { multipart: true } do |f| %>
  <% if @picture.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@picture.errors.count, "error") %> prohibited this picture from being saved:</h2>

      <ul>
      <% @picture.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <!-- <div class="field">
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </div> -->
  <div class="field">
    <%= f.label :image %>
  </div>
  <div class="field">
    <%= f.file_field :image %>
  </div>
  <div class="actions">
    <%= f.submit "Submit" , style: "color:white", class: "btn stylish-color btn-block" %>
  </div>
</div>
</div>
<% end %>
</div>
